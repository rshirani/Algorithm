
public class Program {

	public static void main(String[] args){
		
		Node<Integer> root = new Node<Integer>(2,
				new Node<Integer>(3,
						new Node<Integer>(5,null,new Node<Integer>(6)),
						null),
				new Node<Integer>(4,null,new Node<Integer>(9)));
		System.out.println(Utility.minDepth(root));
	}
}
